@model List<EntityLayer.Concrete.About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımda Veri Listesi</h2>

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif Yap</th>
        <th>Pasif Yap</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>Başlık</td>
            <td>
                <a href="#" class="btn btn-success">Activate</a>
            </td>
            <td>
                <a href="#" class="btn btn-danger">Deactive</a>
            </td>
        </tr>
    }
</table>

@Html.Action("AboutPartial", "About")